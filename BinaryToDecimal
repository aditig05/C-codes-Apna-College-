#include <iostream>
using namespace std;

int BinaryToDecimal(int n){
    int ans = 0;
    int x = 1;
    while(n>0){
        int y = n%10; // for the last digit of each number
        ans += x*y;  // multilpying powrers of 2 in last digit
        x*=2;        //and each time increasing power by 2
        n /= 10;
    }
    return ans;
}
    

int main() {
    int n;
    cin >>n;
    cout<<BinaryToDecimal(n);
	return 0;
}
