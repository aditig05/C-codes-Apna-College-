#include <iostream>
using namespace std;
//inverted number pattern
int main() {
    int n;
    cin>>n; 
        for(int i=1 ; i<=n; i++){
            for(int j=1 ; j<=(n+1-i); j++){ // calculate the no. of rows and columns first. Rows=1 to n Columns=1 to n+1-row no. and elements=j                    cout<<j;  
            }
            cout<<endl; 
    }   
    
    
	return 0;
}
