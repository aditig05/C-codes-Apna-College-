#include <iostream>
#include <math.h>
using namespace std;
// nCr (function)

int fact(int n){
        int factorial=1;
        for(int i=1; i<=n; i++){
            factorial*=i;
            }
    return factorial;
}
            
int main() {
    
    int n,r;
    cin>>n>>r;
    int nCr=fact(n)/(fact(n-r)*fact(r));
    cout<<nCr;
    
	return 0;
}
