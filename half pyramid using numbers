#include <iostream>
using namespace std;
//half pyramid using numbers
int main() {
    int n;
    cin>>n; 
        for(int i=1 ; i<=n; i++){
             for(int j=1 ; j<=i; j++){
                    cout<<i; // for writing numbers from 1 22 333 4444 and so on we use the value of i
            }
            cout<<endl; //to break the line and go to the next line
    }   
	return 0;
}
