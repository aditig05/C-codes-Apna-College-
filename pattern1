#include <iostream>
using namespace std;

//to make a simple rectangle pattern of given rows and columns
int main() {
    int rows, columns;
    cin>>rows>>columns;
    for(int i=1; i<=rows; i++){
        for(int j=1; j<=columns; j++){
            cout<<"*";
        }
        cout<<endl; //to break the line and go to the next line
    }
	return 0;
}
